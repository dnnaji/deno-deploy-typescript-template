    "hono/cors": "https://deno.land/x/hono@v4.2.6/middleware/cors/index.ts",
    "npm:zod": "https://esm.sh/zod@4",
    "neverthrow": "https://esm.sh/neverthrow@6",
    "@std/log": "https://deno.land/std@0.224.0/log/mod.ts",
    "@std/async/retry": "https://deno.land/std@0.224.0/async/retry.ts",
